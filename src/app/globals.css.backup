@layer base {
  * {
    @apply border-border outline-ring/50;
  }
  body {
    @apply bg-background text-foreground;
  }
  
  /* Override sheet title color to black and remove underlines */
  [data-slot="sheet-title"] {
    color: #000000 !important;
    text-decoration: none !important;
  }
  
  /* Remove underlines from all links globally */
  a {
    text-decoration: none !important;
  }
  
  /* Remove hover underline effects */
  a:hover {
    text-decoration: none !important;
  }
  
  /* Remove underlines from all interactive elements */
  button, 
  [role="button"],
  [data-state],
  [data-slot="accordion-trigger"],
  .accordion-trigger,
  * {
    text-decoration: none !important;
  }
  
  /* Remove underlines on hover for all elements */
  *:hover {
    text-decoration: none !important;
  }
}

/* Input hover and focus effects - Ant Design pattern with cyan colors */
/* Target all input elements and select triggers */
input:hover,
[data-slot="select-trigger"]:hover {
  border-color: var(--dsi-sky) !important;
  border-right-width: 1px !important;
}

input:focus,
[data-slot="select-trigger"]:focus,
[data-slot="select-trigger"]:focus-within,
[data-slot="select-trigger"][data-state="open"] {
  border-color: var(--dsi-sky) !important;
  border-right-width: 1px !important;
  box-shadow: 0 0 0 3px rgba(173, 228, 253, 0.2) !important;
  outline: 0 !important;
}

/* Apply hover/focus effects to outer borders ONLY of ShadcnCounter components */
/* Target the motion.div container that wraps the counter - exclude inner elements */
.flex.items-center.gap-0.border.border-input.rounded-md.flex-1:hover {
  border-color: var(--dsi-sky) !important;
  border-right-width: 1px !important;
}

.flex.items-center.gap-0.border.border-input.rounded-md.flex-1:focus-within {
  border-color: var(--dsi-sky) !important;
  border-right-width: 1px !important;
  box-shadow: 0 0 0 3px rgba(173, 228, 253, 0.2) !important;
  outline: 0 !important;
}

/* Prevent inner counter elements from getting the input styling */
.flex.items-center.gap-0.border.border-input.rounded-md.flex-1 button:hover,
.flex.items-center.gap-0.border.border-input.rounded-md.flex-1 input:hover,
.flex.items-center.gap-0.border.border-input.rounded-md.flex-1 button:focus,
.flex.items-center.gap-0.border.border-input.rounded-md.flex-1 input:focus {
  border-color: transparent !important;
  box-shadow: none !important;
  outline: none !important;
}

/* Shadcn horizontal tabs styling - cyan active border, no hover backgrounds */
[data-radix-tabs-trigger][data-state="active"] {
  background: transparent !important;
  background-color: transparent !important;
  border-bottom: 2px solid var(--dsi-sky) !important;
}

[data-radix-tabs-trigger]:focus {
  border-bottom: 2px solid var(--dsi-sky) !important;
  background: transparent !important;
  background-color: transparent !important;
}

/* Remove yellow hover backgrounds from tabs */
html [data-radix-tabs-trigger]:hover {
  background: transparent !important;
  background-color: transparent !important;
}

/* Dropdown menu items hover - soft light cyan */
[role="option"]:hover {
  background-color: var(--dsi-teal-50) !important;
  border: none !important;
  transition: background-color 0.2s ease !important;
}

/* Dropdown menu items active state */
[role="option"]:active {
  background-color: var(--dsi-teal-100) !important;
  background: var(--dsi-teal-100) !important;
  border: none !important;
  transition: background-color 0.2s ease !important;
}

/* Dropdown menu items selected state - override orange with cyan */
[role="option"][data-state="selected"],
[role="option"][aria-selected="true"],
[data-slot="select-item"][data-state="checked"],
[data-slot="select-item"][aria-selected="true"],
.select-item[data-state="checked"],
.select-item[aria-selected="true"] {
  background-color: var(--dsi-teal-200) !important;
  background: var(--dsi-teal-200) !important;
  border: none !important;
  transition: background-color 0.2s ease !important;
}

/* More aggressive override for select items */
html [role="option"][data-state="selected"],
html [role="option"][aria-selected="true"],
html [data-slot="select-item"][data-state="checked"],
html [data-slot="select-item"][aria-selected="true"] {
  background-color: var(--dsi-teal-200) !important;
  background: var(--dsi-teal-200) !important;
}

/* Location dialog Confirm button styling - cyan-6 when active */
[data-slot="dialog-content"].w-\[480px\] [data-slot="button"]:not(:disabled) {
  background-color: #15aabf !important;
  background: #15aabf !important;
  color: white !important;
  border: none !important;
  font-weight: 600 !important;
}

[data-slot="dialog-content"].w-\[480px\] [data-slot="button"]:not(:disabled):hover {
  background-color: #1098ad !important;
  background: #1098ad !important;
  opacity: 1 !important;
}

/* Simple button color change - override primary color and add color variables */
:root {
  --primary: #15aabf;
  --dsi-color-cyan-6: #15aabf;
  --dsi-color-cyan-7: #1098ad;
}
